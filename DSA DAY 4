//pointers with functions

#include<stdio.h>
void swapwithAdress(int* n1, int* n2)
{
int temp;
temp = *n1;
*n1 = *n2;
*n2 = temp;
}
void swapwithoutAddress(int n1, int n2)
{
int temp;
temp = n1;
n1 = n2;
n2 = temp;
}
int main()
{
int num1 = 5, num2 = 10;
 //
swapWhithoutAddress (num1, num2)
printf("After swap w/o addr:\n");
printf("num1=%d,\n", num1);
printf("num1=%d\n\n", num2);

swapWhithtAddress (&num1, &num2)
printf("After swap with addr:\n");
printf("num1=%d,\n", num1);
printf("num1=%d", num2);
return 0;
}
